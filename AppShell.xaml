<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TraningForTwitter.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:TraningForTwitter.View">

    <TabBar Shell.TabBarBackgroundColor= "#E1E1E1" >

            <ShellContent
            Title="Timeline"
            Icon="home.png"
            ContentTemplate="{DataTemplate view:Timelinepage}"/>

            <ShellContent
            Title="Profile"
            Icon="profile.png"
            ContentTemplate="{DataTemplate view:Profilepage}"/>

            <ShellContent
            Title="Intrest"
            Icon="intrest.png"
            ContentTemplate="{DataTemplate view:Intrestpage}"/>

            <ShellContent
            Title="Search"
            Icon="search.png"
            ContentTemplate="{DataTemplate view:Searchpage}"/>
    </TabBar>

</Shell>

